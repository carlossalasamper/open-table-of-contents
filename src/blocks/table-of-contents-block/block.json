{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "open-table-of-contents/table-of-contents",
	"version": "0.1.0",
	"title": "Open Table of Contents",
	"keywords": ["table of contents", "toc", "open toc", "index"],
	"category": "widgets",
	"icon": "info-outline",
	"description": "ðŸ”Ž SEO friendly Table of Contents block for WordPress posts and pages.",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"title": {
			"type": "string",
			"default": "Table of Contents"
		},
		"minHeadingLevel": {
			"type": "integer",
			"default": 2,
			"enum": [2, 3, 4, 5, 6]
		},
		"maxHeadingLevel": {
			"type": "integer",
			"default": 6,
			"enum": [2, 3, 4, 5, 6]
		}
	},
	"textdomain": "open-table-of-contents-table-of-contents",
	"render": "file:./render.php",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
